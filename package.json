  "dependencies": {
    "solid-js": "^1.3.13"
  },
  "devDependencies": {
    "bumpp": "^7.1.1",
    "esbuild": "^0.14.27",
    "esbuild-plugin-solid": "^0.4.2",
    "jsdom": "^19.0.0",
    "solid-testing-library": "^0.3.0",
    "tsup": "^5.12.1",
    "typescript": "^4.4.4",
    "vite-plugin-solid": "^2.2.6",
    "vitest": "^0.7.4"
